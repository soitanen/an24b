<!-- Doors -->

<animation>
  <type>pick</type>
  <object-name>cargodoor</object-name>
  <visible>true</visible>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>nasal</command>
      <script>
        if( getprop("an24/animations/exterior/cargodoor" ) == 1.0 ){
        interpolate("an24/animations/exterior/cargodoor", 0.0, 3.0 );
        }
        else {
        interpolate("an24/animations/exterior/cargodoor", 1.0, 3.0 );
        }
      </script>
    </binding>
  </action>
</animation>

<animation>
  <type>translate</type>
  <object-name>cargodoor</object-name>
  <property>an24/animations/exterior/cargodoor</property>
  <factor>0.1</factor>
  <axis>
    <x>0</x>
    <y>-1</y>
    <z>0</z>    
  </axis>
</animation>

<animation>
  <type>rotate</type>
  <object-name>cargodoor</object-name>
  <property>an24/animations/exterior/cargodoor</property>
  <factor>60</factor>
  <center>
    <x-m>0.0</x-m>
    <y-m>0.0</y-m>
    <z-m>0.0</z-m>
  </center>
  <axis>
    <x>1</x>
    <y>0</y>
    <z>0</z>    
  </axis>
</animation>

