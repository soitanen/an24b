<?xml version="1.0"?>

<!-- Pitch up/down -->

<PropertyList>
 <path>../../shared/switches/switch02.ac</path>
 <animation>
  <type>pick</type>
  <object-name>clickspot_up</object-name>
  <visible>false</visible>
  <action>
    <button>0</button>
    <repeatable>true</repeatable>
    <binding>
      <command>nasal</command>
      <script>
        interpolate("an24/AP-28l1/switches/up-down-l", 1.0, 0.1 );
        setprop("/an24/AP-28l1/internal/kv", 0);
      </script>
    </binding>
    <mod-up>
      <binding>
      <command>nasal</command>
      <script>
        interpolate("an24/AP-28l1/switches/up-down-l", 0.0, 0.1 );
      </script>
    </binding>
    </mod-up>
  </action>
</animation>

 <animation>
  <object-name>clickspot_middle</object-name>
  <type>pick</type>
  <visible>false</visible>
</animation>

 <animation>
  <type>pick</type>
  <object-name>clickspot_down</object-name>
  <visible>false</visible>
  <action>
    <button>0</button>
    <repeatable>true</repeatable>
    <binding>
      <command>nasal</command>
      <script>
        interpolate("an24/AP-28l1/switches/up-down-l", -1, 0.1 );
        setprop("/an24/AP-28l1/internal/kv", 0);
      </script>
    </binding>
    <mod-up>
      <binding>
      <command>nasal</command>
      <script>
        interpolate("an24/ap-28l1/switches/up-down-l", 0.0, 0.1 );
      </script>
    </binding>
    </mod-up>
  </action>
</animation>

 <animation>
   <type>rotate</type>
   <object-name>lever</object-name>
   <property>an24/AP-28l1/switches/up-down-l</property>
  <interpolation>
    <entry><ind>-1.0</ind>	<dep>-30.0</dep></entry>
    <entry><ind>1.0</ind>	<dep>30.0</dep></entry>
  </interpolation>
  <axis>
    <x>0.0</x>
    <y>-1.0</y>
    <z>0.0</z>
  </axis>
 </animation>
 
</PropertyList>
