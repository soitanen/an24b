<?xml version="1.0"?>

<!-- Roll-mode GIK(1)/GPK(0)/ZK(-1) ? -->

<PropertyList>
 <path>../../shared/switches/switch02.ac</path>
 
 <animation>
  <type>pick</type>
  <object-name>clickspot_up</object-name>
  <visible>false</visible>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>nasal</command>
      <script>
        interpolate("an24/ap-28l1/switches/roll-mode", 1.0, 0.1 );
      </script>
    </binding>
  </action>
</animation>

 <animation>
  <type>pick</type>
  <object-name>clickspot_middle</object-name>
  <visible>false</visible>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>nasal</command>
      <script>
        interpolate("an24/ap-28l1/switches/roll-mode", 0.0, 0.1 );
      </script>
    </binding>
  </action>
 </animation>

 <animation>
  <type>pick</type>
  <object-name>clickspot_down</object-name>
  <visible>false</visible>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>nasal</command>
      <script>
        interpolate("an24/ap-28l1/switches/roll-mode", -1.0, 0.1 );
      </script>
    </binding>
  </action>
</animation>

 <animation>
   <type>rotate</type>
   <object-name>lever</object-name>
   <property>an24/ap-28l1/switches/roll-mode</property>
  <interpolation>
    <entry><ind>1.0</ind>	<dep>-5.0</dep></entry>
    <entry><ind>-1.0</ind>	<dep>-50.0</dep></entry>
  </interpolation>
  <axis>
    <x>0.0</x>
    <y>-1.0</y>
    <z>0.0</z>
  </axis>
 </animation>
 

</PropertyList>
