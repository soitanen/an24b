<?xml version="1.0"?>

<!-- Overhead panel of of RPSN -->

<PropertyList>
 <path>controlpanel_oh.ac</path>

 <animation>
   <type>material</type>
   <object-name>rpsnface</object-name>
   <object-name>rpsnface2</object-name>
   <emission>
     <red>1.0</red>
     <green>0.7</green>
     <blue>0.4</blue>
   <factor-prop>/controls/lighting/overhead_instr</factor-prop>
   </emission>
 </animation>

<model>
