<?xml version="1.0"?>

<!-- Pilot's Unit of RPSN, Block 4P-->

<PropertyList>
 <path>block4p.ac</path>

 <animation>
   <type>slider</type>
   <object-name>bracket</object-name>
   <visible>true</visible>
  <drag-direction>vertical</drag-direction>
   <action>
     <binding>
       <command>property-adjust</command>
       <property>an24/animations/RPSN/drag</property>
       <factor>-0.05</factor>
       <min>0</min>
       <max>1</max>
       <wrap>false</wrap>
     </binding>
   </action>
 </animation>

 <animation>
   <type>translate</type>
   <object-name>block4p</object-name>
   <property>an24/animations/RPSN/drag</property>
   <factor>0.07</factor>
   <axis>
     <x>1.0</x>
     <y>0.0</y>
     <z>0.0</z>
   </axis>
 </animation>

 <animation>
   <type>slider</type>
   <object-name>block4p</object-name>
   <visible>true</visible>
  <drag-direction>left-right</drag-direction>
   <action>
     <binding>
       <command>property-adjust</command>
       <property>an24/animations/RPSN/rotate</property>
       <factor>0.05</factor>
       <min>-1</min>
       <max>1</max>
       <wrap>false</wrap>
     </binding>
   </action>
 </animation>

 <animation>
   <type>rotate</type>
   <object-name>block4p</object-name>
   <property>an24/animations/RPSN/rotate</property>
   <factor>20</factor>
   <axis>
     <x>0.0</x>
     <y>0.0</y>
     <z>1.0</z>
   </axis>
 </animation>

</PropertyList>
