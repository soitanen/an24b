<?xml version="1.0"?>

<!-- Front and rear pumps 463 (producing min 0.16kg/m2 pressure each) of tank-group 2 right on/off -->

<PropertyList>
 <path>../../shared/switches/switch03.ac</path>
 <animation>
  <type>pick</type>
  <object-name>clickspot_up</object-name>
  <visible>false</visible>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>nasal</command>
      <script>
        interpolate("an24/FuelControl/sw0405", 1.0, 0.1 );
      </script>
    </binding>
    <binding>
     <condition>
     <equals>
      <property>an24/AZS/sw0115</property>
      <value>1.0</value>
     </equals>
     </condition>
      <command>nasal</command>
      <script>
        setprop("an24/FuelControl/rrear463_press", 0.16 );
        setprop("an24/FuelControl/rfront463_press", 0.16 );
      </script>
    </binding>
  </action>
</animation>

 <animation>
  <object-name>clickspot_middle</object-name>
  <type>pick</type>
  <visible>false</visible>
</animation>

 <animation>
  <type>pick</type>
  <object-name>clickspot_down</object-name>
  <visible>false</visible>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>nasal</command>
      <script>
        setprop("an24/FuelControl/rrear463_press", 0.0 );
        setprop("an24/FuelControl/rfront463_press", 0.0 );
        interpolate("an24/FuelControl/sw0405", 0.0, 0.1 );
      </script>
    </binding>
  </action>
</animation>

 <animation>
   <type>rotate</type>
   <object-name>lever</object-name>
   <property>an24/FuelControl/sw0405</property>
  <interpolation>
    <entry><ind>0.0</ind>	<dep>-30.0</dep></entry>
    <entry><ind>1.0</ind>	<dep>30.0</dep></entry>
  </interpolation>
  <axis>
    <x>0.0</x>
    <y>-1.0</y>
    <z>0.0</z>
  </axis>
 </animation>

</PropertyList>
