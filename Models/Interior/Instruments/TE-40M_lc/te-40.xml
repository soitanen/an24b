<?xml version="1.0"?>

<!-- TE-40, Tachometer of shaft (?,assumed 40.000rpm=100%) of turbogenerator TG-16-->

<PropertyList>
 <path>te-40.ac</path>

 <animation>
   <type>material</type>
   <object-name>tachneedle</object-name>
   <object-name>tachface</object-name>
   <emission>
     <red>1.0</red>
     <green>1.0</green>
     <blue>1.0</blue>
     <factor-prop>an24/lights/instrlights</factor-prop>
   </emission>
 </animation>

<animation>
  <type>rotate</type>
  <object-name>tachneedle</object-name>
  <property>engines/engine[2]/n2</property>
    <interpolation>
      <entry><ind>	0.0</ind>	<dep>	20.0</dep></entry>
      <entry><ind>	37.5</ind>	<dep>	20.0</dep></entry>
      <entry><ind>	100.00</ind>	<dep>	353.33</dep></entry>
    </interpolation>
  <axis>
    <x>-1.0</x>
    <y>0.0</y>
    <z>0.0</z>
  </axis>
</animation>

<!-- TEXT OSG -->

  <animation>
    <type>select</type>
    <object-name>textosg</object-name>
    <condition>
      <property>sim/panel-hotspots</property>
    </condition>
  </animation>

  <text>
    <name>textosg</name>
    <type type="string">literal</type>
    <text type="string">TE-4M TG-16 RPM</text>
    <draw-text type="bool">true</draw-text>
    <font>Helvetica.txf</font>
    <character-size type="double">0.014</character-size>
    <character-aspect-ratio type="double">1.0</character-aspect-ratio>
    <axis-alignment>yz-plane</axis-alignment>
    <max-height>0.06</max-height>
    <max-width>0.05</max-width>
    <font-resolution>
      <width type="int">64</width>
      <height type="int">64</height>
    </font-resolution>
    <alignment>center-center</alignment>
    <offsets>
      <x-m>  0.005 </x-m>
      <y-m>  0.000 </y-m>
      <z-m>  0.000 </z-m>
    </offsets>
  </text>
  <animation>
    <type>material</type>
    <object-name>textosg</object-name>
    <emission>
      <red>   1.0 </red>
      <green> 1.0 </green>
      <blue>  1.0 </blue>
    </emission>
  </animation>


</PropertyList>
