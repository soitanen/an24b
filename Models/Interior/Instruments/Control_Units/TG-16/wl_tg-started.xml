<?xml version="1.0"?>

<!-- Indicates GTD-16 gas-turbine running -->

<PropertyList>
 <path>../../../shared/lamps/sltsn.ac</path>

<animation>
 <type>material</type>
 <object-name>holder</object-name>
 <object-name>lampon</object-name>
 <texture>../../../shared/lamps/sltsngreenplain.png</texture>
</animation>

<animation>
  <type>select</type>
  <condition>
   <greater-than>
    <property>engines/engine[2]/n2</property>
    <value>60</value>
   </greater-than>
  </condition>
  <object-name>lampon</object-name>
</animation>

</PropertyList>
