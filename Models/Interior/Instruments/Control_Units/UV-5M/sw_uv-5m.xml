<?xml version="1.0"?>

<!-- RV-UM switch, left panel -->

<PropertyList>
 <path>../../../shared/switches/2pp-250.ac</path>

 <animation>
  <type>pick</type>
  <object-name>clickspot_up</object-name>
  <visible>false</visible>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>nasal</command>
      <script>
        interpolate("/instrumentation/radar-altimeter/sw-on", 1.0, 0.1 );
      </script>
    </binding>
  </action>
 </animation>

 <animation>
  <type>pick</type>
  <object-name>clickspot_down</object-name>
  <visible>false</visible>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>nasal</command>
      <script>
        interpolate("/instrumentation/radar-altimeter/sw-on", 0.0, 0.1 );
      </script>
    </binding>
  </action>
 </animation>

 <animation>
   <type>rotate</type>
   <object-name>lever</object-name>
   <property>/instrumentation/radar-altimeter/sw-on</property>
  <interpolation>
    <entry><ind>0.0</ind>	<dep>-30.0</dep></entry>
    <entry><ind>1.0</ind>	<dep>30.0</dep></entry>
  </interpolation>
  <axis>
    <x>0.0</x>
    <y>-1.0</y>
    <z>0.0</z>
  </axis>
  <center>
    <x-m>0.0038</x-m>
    <y-m>0.0</y-m>
    <z-m>0.0</z-m>
  </center>
 </animation>

</PropertyList>
