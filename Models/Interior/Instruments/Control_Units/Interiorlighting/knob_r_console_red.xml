<?xml version="1.0"?>

<!-- Right console red lighting -->

<PropertyList>
 <path>../../../shared/switches/rsks-50.ac</path>

 <animation>
   <type>knob</type>
   <object-name>rsks-50</object-name>
   <property>/controls/lighting/console-r_red</property>
   <factor>-100</factor>
  <axis>
    <x>1.0</x>
    <y>0.0</y>
    <z>0.0</z>
  </axis>
    <action>
     <binding>
      <command>property-adjust</command>
      <property>/controls/lighting/console-r_red</property>
      <factor>-0.05</factor>
      <min>0</min>
      <max>1</max>
      <wrap>false</wrap>
     </binding>
    </action>
 </animation>

</PropertyList>
