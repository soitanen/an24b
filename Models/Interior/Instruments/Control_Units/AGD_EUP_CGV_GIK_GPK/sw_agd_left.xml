<?xml version="1.0"?>

<!-- AGD ON -->

<PropertyList>
 <path>../../../shared/switches/2vg-15k.ac</path>
 <animation>
  <type>pick</type>
  <object-name>clickspot_up</object-name>
  <visible>false</visible>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
    <condition>
     <equals>
    	<property>an24/AZS/sw0703</property>
     <value>1.0</value>
     </equals>
    </condition>
      <command>nasal</command>
      <script>
        setprop("an24/instrumentation/agd-l", 5.0 );
      </script>
    </binding>
    <binding>
      <command>nasal</command>
      <script>
        interpolate("an24/instrumentation/sw_agd-l", 1.0, 0.1 );
      </script>
    </binding>
  </action>
</animation>

 <animation>
  <type>pick</type>
  <object-name>clickspot_down</object-name>
  <visible>false</visible>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>nasal</command>
      <script>
        setprop("an24/instrumentation/agd-l", 0.0 );
        interpolate("an24/instrumentation/sw_agd-l", 0.0, 0.1 );
      </script>
    </binding>
  </action>
</animation>

 <animation>
   <type>rotate</type>
   <object-name>lever</object-name>
   <property>an24/instrumentation/sw_agd-l</property>
  <interpolation>
    <entry><ind>0.0</ind>	<dep>-30.0</dep></entry>
    <entry><ind>1.0</ind>	<dep>30.0</dep></entry>
  </interpolation>
  <axis>
    <x>0.0</x>
    <y>-1.0</y>
    <z>0.0</z>
  </axis>
 </animation>

</PropertyList>
