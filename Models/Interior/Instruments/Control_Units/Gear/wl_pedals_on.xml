<?xml version="1.0"?>

<!-- Nosewheel steering by pedals ON -->

<PropertyList>
 <path>../../../shared/lamps/warninglamp03.ac</path>

<animation>
 <type>material</type>
 <object-name>holder</object-name>
 <object-name>lampon</object-name>
 <texture>../../../shared/lamps/warninglamp03green.png</texture>
</animation>

<animation>
  <type>select</type>
  <condition> <!-- Add AZS #5 in future -->
    <and>
      <equals>
        <property>an24/gear/steer_mode</property>
        <value>2</value>
      </equals>
      <equals>
        <property>gear/gear/wow</property>
        <value>1</value>
      </equals>
    </and>
  </condition>
  <object-name>lampon</object-name>
</animation>

</PropertyList>
