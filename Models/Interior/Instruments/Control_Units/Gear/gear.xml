<?xml version="1.0"?>

<!-- Nosewheel steering, handle and/or pedals; anti-skid -->

<PropertyList>

<!-- Gear up/down -->

<model>
 <path>sw_gear-lever.xml</path>
  <offsets>
    <pitch-deg>-43</pitch-deg>
    <x-m>2.633</x-m>
    <y-m>0.131</y-m>
    <z-m>0.56</z-m>
  </offsets>
</model>

<model>
 <path>gear-lever-lock.xml</path>
  <offsets>
    <pitch-deg>43</pitch-deg>
    <x-m>2.633</x-m>
    <y-m>0.131</y-m>
    <z-m>0.56</z-m>
  </offsets>
</model>

<!-- Steering modes -->

<model>
 <path>sw_steering-mode.xml</path>
  <offsets>
    <pitch-deg>-25</pitch-deg>
    <x-m>2.125</x-m>
    <y-m>-0.86256</y-m>
    <z-m>0.668</z-m>
  </offsets>
</model>

<model>
 <path>wl_handle_on.xml</path>
  <offsets>
    <x-m>2.102</x-m>
    <y-m>-0.885</y-m>
    <z-m>0.715</z-m>
  </offsets>
</model>

<model>
 <path>wl_pedals_armed.xml</path>
  <offsets>
    <x-m>2.102</x-m>
    <y-m>-0.86</y-m>
    <z-m>0.715</z-m>
  </offsets>
</model>

<model>
 <path>wl_pedals_on.xml</path>
  <offsets>
    <x-m>2.102</x-m>
    <y-m>-0.835</y-m>
    <z-m>0.715</z-m>
  </offsets>
</model>

<!-- Tiller (steering wheel) -->

<model>
 <path>steering-wheel.xml</path>
  <offsets>
    <x-m>2.6</x-m>
    <y-m>-0.87</y-m>
    <z-m>0.538</z-m>
  </offsets>
</model>

<!-- Brakes - Anti-Skid -->

<model>
 <path>sw_anti-skid.xml</path>
  <offsets>
    <pitch-deg>-25</pitch-deg>
    <x-m>2.117</x-m>
    <y-m>-0.2744</y-m>
    <z-m>0.6859</z-m>
  </offsets>
</model>

<model>
 <path>wl_anti-skid_left.xml</path>
  <offsets>
  <pitch-deg>-25</pitch-deg>
    <x-m>2.112</x-m>
    <y-m>-0.3024</y-m>
    <z-m>0.6972</z-m>
  </offsets>
</model>

<model>
 <path>wl_anti-skid_right.xml</path>
  <offsets>
  <pitch-deg>-25</pitch-deg>
    <x-m>2.112</x-m>
    <y-m>-0.2449</y-m>
    <z-m>0.6972</z-m>
  </offsets>
</model>

</PropertyList>
