<?xml version="1.0"?>

<!-- Steering wheel on left console -->

<PropertyList>
 <path>steering-wheel.ac</path>

 <animation>
   <type>rotate</type>
   <object-name>tiller</object-name>
   <property>/fdm/jsbsim/fcs/tiller-cmd-norm</property>
    <factor>45</factor>
  <axis>
    <x>0.0</x>
    <y>0.0</y>
    <z>-1.0</z>
  </axis>
 </animation>

</PropertyList>
