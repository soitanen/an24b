<?xml version="1.0"?>

<PropertyList>
 <path>uvid-15f.ac</path>

 <animation>
   <type>material</type>
   <object-name>housing</object-name>
   <emission>
     <red>0.8</red>
     <green>0.3</green>
     <blue>0.3</blue>
     <factor-prop>/controls/lighting/panel-m_red</factor-prop>
   </emission>
 </animation>

 <animation>
  <type>material</type>
  <object-name>dig_a_dec</object-name>
  <object-name>dig_a_hund</object-name>
  <object-name>dig_a_ths</object-name>
  <object-name>dig_a_decths</object-name>
  <object-name>dig_p_ones</object-name>
  <object-name>dig_p_dec</object-name>
  <object-name>dig_p_hund</object-name>
  <object-name>dig_p_ths</object-name>
  <object-name>needle</object-name>
   <emission>
     <red>0.7</red>
     <green>0.2</green>
     <blue>0.2</blue>
     <factor-prop>/controls/lighting/panel-m_red</factor-prop>
   </emission>
</animation>

<!-- Needles -->

<animation>
    <type>rotate</type>
    <object-name>needle</object-name>
    <property>/instrumentation/altimeter[1]/indicated-altitude-ft</property>
    <factor>0.36</factor>
    <axis>
      <x>-1.0</x>
      <y>0.0</y>
      <z>0.0</z>
    </axis>
</animation>

<!-- Alt digits -->

