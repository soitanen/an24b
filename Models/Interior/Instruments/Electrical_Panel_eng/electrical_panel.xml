<?xml version="1.0"?>

<PropertyList>
 <path>electrical_panel.ac</path>

<!-- Panel lighting -->

<model>
 <path>knob_panel-lighting.xml</path>
  <offsets>
    <x-m>0.004</x-m>
    <y-m>-0.11</y-m>
    <z-m>0.21</z-m>
  </offsets>
</model>

 <animation>
   <type>material</type>
   <object-name>lightplate</object-name>
   <emission>
     <red>1.0</red>
     <green>0.5</green>
     <blue>0.5</blue>
     <factor-prop>/controls/lighting/panel-elec_red</factor-prop>
   </emission>
 </animation>



 <animation>
   <type>material</type>
   <object-name>ar1_light</object-name>
    <condition>
    <equals>
    <property>an24/Electrical_Panel/sw_pt-1000</property>
    <value>1.0</value>
    </equals>
    </condition>
   <diffuse>
     <red>1.0</red>
     <green>0.0</green>
     <blue>0.0</blue>
   </diffuse>
   <ambient>
     <red>1.0</red>
     <green>0.3</green>
     <blue>0.3</blue>
   </ambient>
   <emission>
     <red>1.0</red>
     <green>0.0</green>
     <blue>0.0</blue>
   </emission>
   <specular>
     <red>1.0</red>
     <green>0.3</green>
     <blue>0.3</blue>
   </specular>
 </animation>

 <animation>
   <type>material</type>
   <object-name>ar2_light</object-name>
    <condition>
    <equals>
    <property>an24/Electrical_Panel/sw_pt-1000</property>
    <value>1.0</value>
    </equals>
    </condition>
   <diffuse>
     <red>1.0</red>
     <green>0.0</green>
     <blue>0.0</blue>
   </diffuse>
   <ambient>
     <red>1.0</red>
     <green>0.3</green>
     <blue>0.3</blue>
   </ambient>
   <emission>
     <red>1.0</red>
     <green>0.0</green>
     <blue>0.0</blue>
   </emission>
   <specular>
     <red>1.0</red>
     <green>0.3</green>
     <blue>0.3</blue>
   </specular>
 </animation>

 <animation>
   <type>material</type>
   <object-name>gs24_light</object-name>
    <condition>
    <equals>
    <property>an24/Electrical_Panel/sw_pt-1000</property>
    <value>1.0</value>
    </equals>
    </condition>
   <diffuse>
     <red>1.0</red>
     <green>0.0</green>
     <blue>0.0</blue>
   </diffuse>
   <ambient>
     <red>1.0</red>
     <green>0.3</green>
     <blue>0.3</blue>
   </ambient>
   <emission>
     <red>1.0</red>
     <green>0.0</green>
     <blue>0.0</blue>
   </emission>
   <specular>
     <red>1.0</red>
     <green>0.3</green>
     <blue>0.3</blue>
   </specular>
 </animation>


