<?xml version="1.0"?>

<!-- PO-750 on/ off/ AUX 115V/400Hz -->

<PropertyList>
 <path>../../shared/switches/ppng-15k.ac</path>

 <animation>
  <type>pick</type>
  <object-name>clickspot_up</object-name>
  <visible>false</visible>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>nasal</command>
      <script>
        interpolate("an24/Electrical_Panel/sw_PO-750_AUX", 1.0, 0.1 );
      </script>
    </binding>
  </action>
 </animation>

 <animation>
  <type>pick</type>
  <object-name>clickspot_middle</object-name>
  <visible>false</visible>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>nasal</command>
      <script>
        interpolate("an24/Electrical_Panel/sw_PO-750_AUX", 0.0, 0.1 );
      </script>
    </binding>
  </action>
 </animation>

 <animation>
  <type>pick</type>
  <object-name>clickspot_down</object-name>
  <visible>false</visible>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>nasal</command>
      <script>
        interpolate("an24/Electrical_Panel/sw_PO-750_AUX", -1.0, 0.1 );
      </script>
    </binding>
  </action>
 </animation>

 <animation>
   <type>rotate</type>
   <object-name>lever</object-name>
   <property>an24/Electrical_Panel/sw_PO-750_AUX</property>
  <interpolation>
    <entry><ind>1.0</ind>	<dep>20.0</dep></entry>
    <entry><ind>-1.0</ind>	<dep>-20.0</dep></entry>
  </interpolation>
  <axis>
    <x>0.0</x>
    <y>-1.0</y>
    <z>0.0</z>
  </axis>
 </animation>
</PropertyList>
