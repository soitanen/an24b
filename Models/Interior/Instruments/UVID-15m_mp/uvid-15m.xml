<!--Controls-->
<animation>
  <type>rotate</type>
  <object-name>Knob</object-name>
  <property>instrumentation/altimeter/setting-inhg</property>  
  <factor>300</factor>
  <center>
    <x-m>0.0</x-m>
    <y-m>0.02862</y-m>
    <z-m>-0.02885</z-m>
  </center>
  <axis>
    <x>1.0</x>
    <y>0.0</y>
    <z>0.0</z>
  </axis>
</animation>
<!--Mouse wheel-->
<animation>
  <type>pick</type>
  <object-name>Knob</object-name>
  <visible>true</visible>
  <action>
    <button>3</button>
    <repeatable>false</repeatable>
    <binding>
      <command>property-adjust</command>
      <property>instrumentation/altimeter/setting-inhg</property>
      <step>0.01</step>
      <min>10.0</min>
      <max>40.0</max>
    </binding>
  </action>
</animation>
<animation>
  <type>pick</type>
  <object-name>Knob</object-name>
  <visible>true</visible>
  <action>
    <button>4</button>
    <repeatable>false</repeatable>
    <binding>
      <command>property-adjust</command>
      <property>instrumentation/altimeter/setting-inhg</property>
      <step>-0.01</step>
      <min>10.0</min>
      <max>40.0</max>
    </binding>
  </action>
</animation>

