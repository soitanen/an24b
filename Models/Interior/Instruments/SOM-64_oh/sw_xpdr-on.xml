<?xml version="1.0"?>

<!-- Transponder ON/OFF -->

<PropertyList>
 <path>../../shared/switches/2png-15k.ac</path>

 <animation>
  <type>pick</type>
  <object-name>clickspot_up</object-name>
  <visible>false</visible>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>nasal</command>
      <script>
        interpolate("/instrumentation/transponder/serviceable", 1.0, 0.1 );
        setprop("/instrumentation/transponder/inputs/serviceable", 1.0 );
      </script>
    </binding>
  </action>
 </animation>

<animation>
  <type>pick</type>
  <object-name>clickspot_down</object-name>
  <visible>false</visible>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>nasal</command>
      <script>
        interpolate("/instrumentation/transponder/serviceable", 0.0, 0.1 );
        setprop("/instrumentation/transponder/inputs/serviceable", 0.0 );
      </script>
    </binding>
  </action>
</animation>

 <animation>
   <type>rotate</type>
   <object-name>lever</object-name>
   <property>/instrumentation/transponder/serviceable</property>
  <interpolation>
    <entry><ind>0.0</ind>	<dep>-20.0</dep></entry>
    <entry><ind>1.0</ind>	<dep>20.0</dep></entry>
  </interpolation>
  <axis>
    <x>0.0</x>
    <y>-1.0</y>
    <z>0.0</z>
  </axis>
 </animation>


</PropertyList>
