<PropertyList>

<!-- 115/36V AC panel, from top to bottom rows (01xx-07xx) and left to right (xx01-xx08);
functionality not to be taken too seriously atm! -->

  <path>115_36VAC_panel.ac</path>

<animation>
  <type>pick</type>
  <object-name>fuse0101</object-name>
  <visible>true</visible>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>nasal</command>
      <script>
        if( getprop("an24/animations/fuses/fuse0101" ) == 0.0 ){
        interpolate("an24/animations/fuses/fuse0101", 3.0, 3.0 );
        }
        else {
        interpolate("an24/animations/fuses/fuse0101", 0.0, 3.0 );
        }
      </script>
    </binding>
  </action>
</animation>

 <animation>
   <type>rotate</type>
   <object-name>fuse0101</object-name>
   <property>an24/animations/fuses/fuse0101</property>
    <interpolation>
      <entry>
	<ind>0.0</ind>
	<dep>0.0</dep>
      </entry>
      <entry>
	<ind>2.5</ind>
	<dep>720.0</dep>
      </entry>
      <entry>
	<ind>3.0</ind>
	<dep>720.0</dep>
      </entry>
    </interpolation>
    <center>
     <x-m>0</x-m>
     <y-m>-0.116</y-m>
     <z-m>0.2</z-m>
    </center>
    <axis>
     <x>1</x>
     <y>0</y>
     <z>0</z>
    </axis>
 </animation>

 <animation>
   <type>rotate</type>
   <object-name>fuse0101</object-name>
   <property>an24/animations/fuses/fuse0101</property>
    <interpolation>
      <entry>
	<ind>0.0</ind>
	<dep>0.0</dep>
      </entry>
      <entry>
	<ind>2.5</ind>
	<dep>0.0</dep>
      </entry>
      <entry>
	<ind>3.0</ind>
	<dep>30.0</dep>
      </entry>
    </interpolation>
    <center>
     <x-m>-0.79</x-m>
     <y-m>0</y-m>
     <z-m>-0.27</z-m>
    </center>
    <axis>
     <x>0</x>
     <y>1</y>
     <z>0</z>
    </axis>
 </animation>



</PropertyList>
