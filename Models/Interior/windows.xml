<animation>
  <object-name>Lwindow_ice</object-name>
  <type>select</type>
  <condition>
   <greater-than>
    <property>an24/Anti-Ice/elements/windowL_cm</property>
    <value>0.0</value>
   </greater-than>
  </condition>
</animation>


<animation>
  <type>blend</type>
  <object-name>Lwindow_ice</object-name>
  <property>an24/Anti-Ice/elements/windowL_cm</property>
    <interpolation>
     <entry>
      <ind>1.5</ind> <dep>0.0</dep>
     </entry>
     <entry>
      <ind>0.0</ind> <dep>1.0</dep>
     </entry>
    </interpolation>
  <!--min>0.0</min>
  <max>1.0</max-->
</animation>

