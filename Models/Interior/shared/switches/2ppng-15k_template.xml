<?xml version="1.0"?>

<!-- Template/example -->

<PropertyList>
 <path>../../../shared/switches/2ppng-15k.ac</path>

 <animation>
  <type>pick</type>
  <object-name>clickspot_up</object-name>
  <visible>false</visible>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
        <condition><!-- optional -->
         <equals>
         <property>an24/AZS/swxxyy</property>
         <value>1.0</value>
         </equals>
        </condition>
      <command>nasal</command>
      <script>
        interpolate("foo/bar/prop_to_be_changed", 1.0, 0.4 );
      </script>
    </binding>
<!-- animation, begin with sw_ -->
    <binding>
      <command>nasal</command>
      <script>
	interpolate("foo/bar/sw_prop_to_be_changed", 1.0, 0.2 );
      </script>
    </binding>
  </action>
 </animation>

