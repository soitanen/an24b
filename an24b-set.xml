<input>
 <keyboard>

     <key n="115">
       <name>s</name>
       <desc>Autostart</desc>
       <binding>
        <command>nasal</command>
        <script>autostart.startup();</script>
       </binding>
     </key>

     <key n="258">
       <name>F2</name>
       <desc>Prop pitch lock</desc>
       <binding>
        <condition>
         <equals>
         <property>an24/AZS/sw0203</property>
         <value>1.0</value>
         </equals>
        </condition>
         <command>property-toggle</command>
         <property>/an24/instrumentation/prop-lock/prop-lock-left</property>
       </binding>
       <binding>
        <condition>
         <equals>
         <property>an24/AZS/sw0203</property>
         <value>1.0</value>
         </equals>
        </condition>
         <command>property-toggle</command>
         <property>/an24/instrumentation/prop-lock/prop-lock-right</property>
       </binding>
     </key>

     <key n="257">
       <name>F1</name>
       <desc>Idle lock</desc>
       <binding>
          <command>property-assign</command>
          <property>/an24/instrumentation/idle-lock/idle-lock</property>
          <value>1</value>
       </binding>
       <mod-up>
         <binding>
           <command>property-assign</command>
           <property>/an24/instrumentation/idle-lock/idle-lock</property>
           <value>0</value>
         </binding>            
       </mod-up>
     </key>

      <key n="45">
      <name>minus</name>
      <binding>
        <command>property-adjust</command>
        <property>/fdm/jsbsim/fcs/throttle/idle-lock-pos</property>
        <step>-0.02</step>
      </binding>
    </key>

     <key n="61">
      <name>equal</name>
      <binding>
        <command>property-adjust</command>
        <property>/fdm/jsbsim/fcs/throttle/idle-lock-pos</property>
        <step>0.02</step>
      </binding>
     </key>

     <key n="91">
        <name>[</name>
        <desc>Decrease flaps</desc>
        <binding>
          <command>nasal</command>
          <script>
            var flaps_min = 0;
            setprop("/controls/flight/flaps", flaps_min);
          </script>
        </binding>
        <mod-up>
          <binding>
            <command>nasal</command>
            <script>
              var flaps_current = getprop("/surface-positions/flap-pos-norm");
              setprop("/controls/flight/flaps", flaps_current);
            </script>
          </binding>
        </mod-up>
     </key>

      <key n="93">
        <name>]</name>
        <desc>Increase flaps</desc>
        <binding>
          <command>nasal</command>
          <script>
            var flaps_max = 1;
            setprop("/controls/flight/flaps", flaps_max);
          </script>
        </binding>
        <mod-up>
          <binding>
            <command>nasal</command>
            <script>
              var flaps_current = getprop("/surface-positions/flap-pos-norm");
              setprop("/controls/flight/flaps", flaps_current);
            </script>
          </binding>
        </mod-up>
      </key>

<!-- For gear switch animation only -->

     <key n="103">
      <name>g</name>
      <binding>
       <command>nasal</command>
       <script>
	controls.gearDown(-1);
        interpolate("/an24/animations/gear-lever", 1.0, 0.2, 1.0, 5.0, 0.0, 0.2 );
       </script>
      </binding>
     </key>

     <key n="71">
      <name>G</name>
      <binding>
       <command>nasal</command>
       <script>
	controls.gearDown(1);
        interpolate("/an24/animations/gear-lever", -1.0, 0.2, -1.0, 5.0, 0.0, 0.2 );
       </script>
      </binding>
     </key>

      <key n="100">
      <name>d</name>
      <desc>AP (DIS-)CONNECT</desc>
      <repeatable>false</repeatable>
       <binding>
       <command>nasal</command>
        <script>
         if( getprop("an24/AP-28l1/internal/engaged" ) == 0.0 ){
         instruments.engage_ap();
         }
         else {
         setprop("an24/AP-28l1/internal/kv", 0.0 );
         setprop("an24/AP-28l1/internal/horizon-mode", 0.0 );
         setprop("an24/AP-28l1/internal/engaged", 0.0 );
         }	
        </script>
       </binding>
      </key>

     <key n="60">
       <name>less-than</name>
       <desc>Overhead View</desc>
       <binding>
        <command>nasal</command>
        <script>
	setprop("/sim/current-view/heading-offset-deg2", getprop("/sim/current-view/heading-offset-deg") );
	setprop("/sim/current-view/pitch-offset-deg2", getprop("/sim/current-view/pitch-offset-deg") );
	setprop("/sim/current-view/field-of-view2", getprop("/sim/current-view/field-of-view") );
	</script>
       </binding>
       <binding>
          <command>property-assign</command>
          <property>/sim/current-view/goal-pitch-offset-deg</property>
          <value>89.8</value>
       </binding>
       <binding>
          <command>property-assign</command>
          <property>/sim/current-view/field-of-view</property>
          <value>90</value>
       </binding>
       <binding>
          <command>property-assign</command>
          <property>/sim/current-view/x-offset-m</property>
          <value>-0.11217</value>
       </binding>
       <binding>
          <command>property-assign</command>
          <property>/sim/current-view/y-offset-m</property>
          <value>0.954385</value>
       </binding>
       <binding>
          <command>property-assign</command>
          <property>/sim/current-view/z-offset-m</property>
          <value>-6.0119247</value>
       </binding>
       <mod-up>
       <binding>
        <command>nasal</command>
        <script>
	if (getprop("/sim/current-view/view-number") > 7 ) {
	var viewnr = getprop("/sim/current-view/view-number") + 93 ;
	}
	else {
	var viewnr = getprop("/sim/current-view/view-number");
	}
	var offsetx = getprop("/sim/view[" ~ viewnr ~ "]/config/x-offset-m");
	var offsety = getprop("/sim/view[" ~ viewnr ~ "]/config/y-offset-m");
	var offsetz = getprop("/sim/view[" ~ viewnr ~ "]/config/z-offset-m");
	setprop("/sim/current-view/x-offset-m", offsetx);
	setprop("/sim/current-view/y-offset-m", offsety);
	setprop("/sim/current-view/z-offset-m", offsetz);
	setprop("/sim/current-view/heading-offset-deg", getprop("/sim/current-view/heading-offset-deg2") );
	setprop("/sim/current-view/pitch-offset-deg", getprop("/sim/current-view/pitch-offset-deg2") );
	setprop("/sim/current-view/field-of-view", getprop("/sim/current-view/field-of-view2") );
	</script>
       </binding>
       </mod-up>
      </key>

    <key n="49">
      <name>1</name>
      <desc>Captain view</desc>
      <binding>
        <command>nasal</command>
        <script>view.forceView(0);</script>
