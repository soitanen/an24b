  <help>
   <title>An-24B</title>
   	   <key>
	    <name>F1</name>
	    <desc>Release Idle Lock (press for release)</desc>
	   </key>
   	   <key>
	    <name>Del</name>
	    <desc>Toggles Propeller Pitch Lock (UPOR VINTA)</desc>
	   </key>
   	   <key>
	    <name>-</name>
	    <desc>Decrease Idle Lock value</desc>
	   </key>
   	   <key>
