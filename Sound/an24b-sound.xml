<!-- Negative thrust sound -->
  <propeller_rev>
   <name>Right engine propeller negative thrust external</name>
   <mode>looped</mode>
   <path>Aircraft/an24b/Sound/Engine/AI-24_xProp1-1.wav</path>
   <condition>
      <not><property>sim/current-view/internal</property></not>
     <property>engines/engine[1]/running</property>
    <less-than>
      <property>/engines/engine[1]/thrust_lb</property>
      <value>0</value>
    </less-than>
   </condition>
   <volume>
    <type>abs</type>
    <property>/engines/engine[1]/thrust_lb</property>
    <factor>0.00125</factor>
   </volume>
   <pitch>
    <property>/engines/engine[0]/thruster/rpm</property>
    <factor>0.000873362</factor>
    <min>0.5</min>
    <max>2.0</max>
    <offset>0.0</offset>
   </pitch>
  <position>
    <x> 5.94 </x>
    <y> -3.95 </y>
    <z> 1.185 </z>
  </position>
  </propeller_rev>

  <rumble>
   <name>rumble</name>
   <mode>looped</mode>
   <path>Sounds/rumble.wav</path>
   <condition>
    <or>
     <property>/gear/gear[0]/wow</property>
     <property>/gear/gear[1]/wow</property>
     <property>/gear/gear[2]/wow</property>
    </or>
    <!-- <property>/sim/current-view/internal</property> -->
   </condition>
   <volume>
    <property>/velocities/airspeed-kt</property>
    <type>log</type>
    <factor>0.25</factor>
   </volume>
  </rumble>


<gear>
  <name>gear up</name>
  <path>Aircraft/an24b/Sound/Systems/24gear.wav</path>
  <condition>
    <less-than>
      <property>gear/gear[1]/position-norm</property>
      <value>0.9</value>
    </less-than>
  </condition>
  <mode>once</mode>
</gear> 

<gear>
  <name>gear down</name>
  <path>Aircraft/an24b/Sound/Systems/24gear.wav</path>
  <condition>
    <greater-than>
      <property>gear/gear[1]/position-norm</property>
      <value>0.1</value>
    </greater-than>
  </condition>
  <mode>once</mode>
</gear> 
<!-- 
  <gear>
   <name>gear-lock</name>
   <path>Sounds/gear-lck.wav</path>
   <condition>
    <greater-than>
     <property>/gear/gear[0]/position-norm</property>
     <value>0.9</value>
    </greater-than>
   </condition>
  </gear>
-->

  <squeal>
   <name>squeal</name>
   <path>Sounds/squeal.wav</path>
   <condition>
    <or>
     <property>/gear/gear[0]/wow</property>
     <property>/gear/gear[1]/wow</property>
     <property>/gear/gear[2]/wow</property>
    </or>
   </condition>
   <volume>
    <property>/velocities/speed-down-fps</property>
    <factor>0.5</factor>
    <max>1.0</max>
   </volume>
   <volume>
    <property>/velocities/airspeed-kt</property>
    <factor>0.01</factor>
    <max>1.0</max>
   </volume>
   <volume>
    <internal>dt_stop</internal>
    <factor>0.05</factor>
    <max>1.0</max>
   </volume>
   <pitch>
    <property>/velocities/airspeed-kt</property>
    <factor>0.0025</factor>
    <offset>1.2</offset>
   </pitch>
  </squeal>
 
  <flap>
   <name>flaps</name>
   <mode>in-transit</mode>
   <path>Aircraft/an24b/Sound/Systems/24flaps.wav</path>
   <property>/surface-positions/flap-pos-norm</property>
   <volume>
    <property>sim/current-view/internal</property>
    <factor>1</factor>
   </volume>
  </flap>

  <wind>
   <name>wind</name>
   <mode>looped</mode>
   <path>Sounds/wind.wav</path>
   <property>/velocities/airspeed-kt</property>
   <volume>
    <property>/position/altitude-ft</property>
    <factor>-0.000015</factor>
    <offset>1.0</offset>
    <min>0.1</min>
    <max>1.0</max>
   </volume>
   <volume>
     <property>/velocities/airspeed-kt</property>
     <factor>0.0015</factor>
     <min>0.03</min>
     <max>0.25</max>
   </volume>
   <pitch>
    <property>/velocities/airspeed-kt</property>
    <factor>0.0035</factor>
    <offset>1.25</offset>
   </pitch>
  </wind>
 
  <stall>
   <name>stall</name>
   <mode>looped</mode>
   <path>Sounds/stall.wav</path>
   <condition>
    <and>
     <property>/sim/alarms/stall-warning</property>
     <greater-than>
       <property>/velocities/airspeed-kt</property>
       <value>10</value>
     </greater-than>
     <not>
       <property>/gear/gear[0]/wow</property>
       <property>/gear/gear[1]/wow</property>
       <property>/gear/gear[2]/wow</property>
     </not>
    </and>
    <!-- <property>/sim/current-view/internal</property> -->
   </condition>
   <volume>
    <property>/sim/alarms/stall-warning</property>
    <max>1.0</max>
   </volume>
   <reference-dist>10.0</reference-dist>
   <max-dist>20.0</max-dist>
  </stall>
 
<!-- no binding yet?
  <click>
   <name>click</name>
   <path>Sounds/click.wav</path>
  </click>
-->

 <marker>
    <name>inner</name>
    <path>Aircraft/an24b/Sound/Systems/Innermk.wav</path>
    <condition>
      <property>instrumentation/marker-beacon/inner</property>
      <property>sim/current-view/internal</property>
    </condition>
    <mode>once</mode>
    <volume>
      <offset type="double">0.5</offset>
    </volume>
  </marker>
  <marker>
    <name>middle</name>
    <path>Aircraft/an24b/Sound/Systems/Middmk.wav</path>
    <condition>
      <property>instrumentation/marker-beacon/middle</property>
      <property>sim/current-view/internal</property>
    </condition>
    <mode>once</mode>
    <volume>
      <offset type="double">0.5</offset>
    </volume>
  </marker>
  
  <marker>
    <name>outer</name>
    <path>Aircraft/an24b/Sound/Systems/Outermk.wav</path>
    <condition>
      <property>instrumentation/marker-beacon/outer</property>
      <property>sim/current-view/internal</property>
    </condition>
    <mode>once</mode>
    <volume>
      <offset type="double">0.5</offset>
    </volume>
  </marker>

</fx>
 
</PropertyList>
