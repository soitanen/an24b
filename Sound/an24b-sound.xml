<?xml version="1.0"?>
 
<PropertyList>

 <fx>
 
  <crank>
   <name>engstart</name>
   <path>Sounds/engstart.wav</path>
   <property>/engines/engine/cranking</property>
   <volume>
    <offset>0.4</offset>
   </volume>
  </crank>

  <crank>
   <name>crank</name>
   <mode>looped</mode>
   <path>Sounds/cranking.wav</path>
   <condition>
    <property>/engines/engine/cranking</property>
    <not>
     <property>/engines/engine/running</property>
    </not>
   </condition>
   <volume>
    <offset>0.5</offset>
   </volume>
  </crank>

  <crank>
   <name>cough</name>
   <path>Sounds/coughing.wav</path>
   <property>/engines/engine/running</property>
   <volume>
    <offset>0.3</offset>
   </volume>
  </crank>

<!-- Engine internal sound -->
<!-- Left engine -->
  <propeller>
   <name>Left engine internal</name>
   <mode>looped</mode>
   <path>Aircraft/an24b/Sound/Engine/AI-24_Prop1-2.wav</path>
   <condition>
     <property>sim/current-view/internal</property>
     <property>engines/engine[0]/running</property>
   </condition>
   <volume>
    <property>engines/engine[0]/running</property>
    <factor>0.5</factor>
    <min>0.0</min>
    <max>1.0</max>
   </volume>
   <pitch>
    <property>/engines/engine[0]/thruster/rpm</property>
    <factor>0.000803213</factor>
    <min>0.5</min>
    <max>2.0</max>
    <offset>0.0</offset>
   </pitch>
  <position>
    <x> 5.94 </x>
    <y> 3.95 </y>
    <z> 1.185 </z>
  </position>
  </propeller>
<!-- Negative thrust sound -->
  <propeller_rev>
   <name>Left engine propeller negative thrust internal</name>
   <mode>looped</mode>
   <path>Aircraft/an24b/Sound/Engine/AI-24_Prop1-1.wav</path>
   <condition>
     <property>sim/current-view/internal</property>
     <property>engines/engine[0]/running</property>
    <less-than>
      <property>/engines/engine[0]/thrust_lb</property>
      <value>0</value>
    </less-than>
   </condition>
   <volume>
    <type>abs</type>
    <property>/engines/engine[0]/thrust_lb</property>
    <factor>0.00125</factor>
   </volume>
   <pitch>
    <property>/engines/engine[0]/thruster/rpm</property>
    <factor>0.000873362</factor>
    <min>0.5</min>
    <max>2.0</max>
    <offset>0.0</offset>
   </pitch>
  <position>
    <x> 5.94 </x>
    <y> 3.95 </y>
    <z> 1.185 </z>
  </position>
  </propeller_rev>
<!-- Right engine -->
  <propeller>
   <name>Right engine internal</name>
   <mode>looped</mode>
   <path>Aircraft/an24b/Sound/Engine/AI-24_Prop1-2.wav</path>
   <condition>
     <property>sim/current-view/internal</property>
     <property>engines/engine[1]/running</property>
   </condition>
   <volume>
    <property>engines/engine[1]/running</property>
    <factor>0.5</factor>
    <min>0.0</min>
    <max>1.0</max>
   </volume>
   <pitch>
    <property>/engines/engine[0]/thruster/rpm</property>
    <factor>0.000803213</factor>
    <min>0.5</min>
    <max>2.0</max>
    <offset>0.0</offset>
   </pitch>
  <position>
    <x> 5.94 </x>
    <y> -3.95 </y>
    <z> 1.185 </z>
  </position>
  </propeller>
<!-- Negative thrust sound -->
  <propeller_rev>
   <name>Right engine propeller negative thrust internal</name>
   <mode>looped</mode>
   <path>Aircraft/an24b/Sound/Engine/AI-24_Prop1-1.wav</path>
   <condition>
     <property>sim/current-view/internal</property>
     <property>engines/engine[1]/running</property>
    <less-than>
      <property>/engines/engine[1]/thrust_lb</property>
      <value>0</value>
    </less-than>
   </condition>
   <volume>
    <type>abs</type>
    <property>/engines/engine[1]/thrust_lb</property>
    <factor>0.00125</factor>
   </volume>
   <pitch>
    <property>/engines/engine[0]/thruster/rpm</property>
    <factor>0.000873362</factor>
    <min>0.5</min>
    <max>2.0</max>
    <offset>0.0</offset>
   </pitch>
  <position>
    <x> 5.94 </x>
    <y> -3.95 </y>
    <z> 1.185 </z>
  </position>
  </propeller_rev>

<!-- Engine external sound -->
<!-- Left engine -->
  <propeller>
   <name>Left engine external</name>
   <mode>looped</mode>
   <path>Aircraft/an24b/Sound/Engine/AI-24_xProp1-2.wav</path>
   <condition>
      <not><property>sim/current-view/internal</property></not>
     <property>engines/engine[0]/running</property>
   </condition>
   <volume>
